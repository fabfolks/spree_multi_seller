<div class="orders">
</div>
<div style="padding-top: 50px;">
	<table>
		<thead>
			<tr>
				<th>#</th>
				<th>Completed At</th>
				<th>Order Number</th>
				<th>State</th>
				<th>Payment State</th>
				<th>Shipment State</th>
				<th>Custmer Email</th>
				<th>Total</th>
				<th colspan=3></th>

			</tr>
		</thead>
		<tbody>
			<% @orders.each_with_index do |order, index| %>
				<tr>
					<td><%= index + 1 %></td>
					<td><%= order.order.completed_at%></td>
					<td><%= order.order.number%></td>
					<td><%= order.order.state%></td>
					<td><%= order.order.payment_state%></td>
					<td><%= order.order.shipment_state%></td>
					<td><%= order.order.email%></td>
					<td><%= order.order.total%></td>

				</tr>
			<% end%>
		</tbody>
	</table>
</div>
