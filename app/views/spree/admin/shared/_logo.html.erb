    <figure data-hook="logo-wrapper" class="columns five">
    	<% if spree_current_user.seller %>
	    	<% if spree_current_user.seller.logo %>
	      	<%= link_to image_tag(spree_current_user.seller.logo.url(:mini), :id => 'logo'), spree.admin_path %>
	      <% else %>
	      	<%= link_to image_tag(Spree::Config[:admin_interface_logo], :id => 'logo'), spree.admin_path %>
	      <% end %>
      <% else %>
      	<%= link_to image_tag(Spree::Config[:admin_interface_logo], :id => 'logo'), spree.admin_path %>
      <% end %>
    </figure>